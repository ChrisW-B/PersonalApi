{
  "name": "chriss-personal-site",
  "version": "1.0.0",
  "description": "A Simple Personal Website",
  "keywords": [
    "personal",
    "site"
  ],
  "homepage": "https://github.com/ChrisW-B/PersonalWebsite#readme",
  "bugs": {
    "url": "https://github.com/ChrisW-B/PersonalWebsite/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChrisW-B/PersonalWebsite.git"
  },
  "license": "ISC",
  "author": "Chris Barry <me@chrisb.xyz>",
  "main": "build/index.js",
  "scripts": {
    "build": "netlify-lambda build src/functions --config webpack.config.js",
    "clean": "rm -rf ./build",
    "lint:js": "eslint \"./src/**/*.{t,j}s{x,}\" --ignore-pattern 'build/**/*.js'",
    "lint:js:fix": "npm run lint:js -- --fix",
    "prettier": "prettier --list-different './**/*.{ts,js,jsx,tsx,gql,html,yml,json}' --write",
    "prettier:check": "prettier './**/*.{ts,js,jsx,tsx,gql,html,yml,json}' -c",
    "start": "netlify dev",
    "typescript": "tsc --noEmit"
  },
  "dependencies": {
    "@types/node-fetch": "2.5.12",
    "apollo-server-core": "3.4.1",
    "apollo-server-lambda": "3.4.1",
    "body-parser": "1.19.2",
    "compression": "1.7.4",
    "dotenv": "16.0.0",
    "express": "4.17.3",
    "express-graphql": "0.12.0",
    "graphql": "15.7.2",
    "lastfm-njs": "1.0.0",
    "node-fetch": "2.6.7",
    "twitter": "1.7.1",
    "twitter-text": "3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "7.16.8",
    "@babel/core": "7.16.12",
    "@babel/node": "7.16.8",
    "@babel/plugin-proposal-class-properties": "7.16.7",
    "@babel/preset-env": "7.16.11",
    "@babel/preset-typescript": "7.16.7",
    "@types/aws-lambda": "8.10.93",
    "@types/node": "16.11.26",
    "@types/twitter": "1.7.1",
    "@types/twitter-text": "3.1.1",
    "@typescript-eslint/eslint-plugin": "5.3.1",
    "@typescript-eslint/parser": "5.3.1",
    "@typescript-eslint/typescript-estree": "5.3.1",
    "eslint": "8.1.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-prettier": "4.0.0",
    "import-sort": "6.0.0",
    "import-sort-parser-typescript": "6.0.0",
    "import-sort-style-react": "5.1.0",
    "netlify-cli": "9.4.4",
    "netlify-lambda": "2.0.15",
    "nodemon": "2.0.15",
    "path": "0.12.7",
    "prettier": "2.4.1",
    "prettier-eslint": "13.0.0",
    "prettier-plugin-import-sort": "0.0.7",
    "prettier-plugin-packagejson": "2.2.16",
    "typescript": "4.4.4",
    "webpack-cli": "4.9.2",
    "webpack-node-externals": "3.0.0"
  },
  "importSort": {
    ".js, .jsx, .es6, .es": {
      "parser": "babylon",
      "style": "react"
    },
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "react"
    }
  }
}
