{
  "name": "chriss-personal-site",
  "version": "1.0.0",
  "description": "A Simple Personal Website",
  "keywords": [
    "personal",
    "site"
  ],
  "homepage": "https://github.com/ChrisW-B/PersonalWebsite#readme",
  "bugs": {
    "url": "https://github.com/ChrisW-B/PersonalWebsite/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChrisW-B/PersonalWebsite.git"
  },
  "license": "ISC",
  "author": "Chris Barry <me@chrisb.xyz>",
  "main": "build/index.js",
  "scripts": {
    "build": "netlify-lambda build src/functions --config webpack.functions.js",
    "clean": "rm -rf ./build",
    "lint:js": "eslint \"./src/**/*.{t,j}s{x,}\" --ignore-pattern 'build/**/*.js'",
    "lint:js:fix": "npm run lint:js -- --fix",
    "prettier": "prettier './src/**/*.{ts,js,jsx,tsx,gql,html,yml,json}' --write",
    "prettier:check": "prettier './src/**/*.{ts,js,jsx,tsx,gql,html,yml,json}' -c",
    "start": "netlify dev",
    "typescript": "tsc --noEmit"
  },
  "dependencies": {
    "apollo-server": "^2.16.0",
    "apollo-server-lambda": "^2.16.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-graphql": "^0.11.0",
    "graphql": "^15.3.0",
    "lastfm-njs": "^1.0.0",
    "node-fetch": "^2.6.0",
    "twitter": "^1.7.1",
    "twitter-text": "^3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.10.4",
    "@babel/node": "^7.10.4",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@types/aws-lambda": "^8.10.59",
    "@types/node": "^14.0.23",
    "@types/node-fetch": "^2.5.7",
    "@types/twitter": "^1.7.0",
    "@types/twitter-text": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "@typescript-eslint/typescript-estree": "^3.7.0",
    "eslint": "^7.5.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-import-resolver-webpack": "^0.12.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-unicorn": "^20.1.0",
    "import-sort": "^6.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-react": "^5.1.0",
    "netlify-cli": "^2.58.0",
    "netlify-lambda": "^1.6.3",
    "nodemon": "^2.0.4",
    "prettier": "^2.0.5",
    "prettier-eslint": "^11.0.0",
    "prettier-plugin-import-sort": "0.0.4",
    "prettier-plugin-packagejson": "^2.2.5",
    "typescript": "^3.9.7",
    "webpack-node-externals": "^2.5.0"
  },
  "importSort": {
    ".js, .jsx, .es6, .es": {
      "parser": "babylon",
      "style": "react"
    },
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "react"
    }
  }
}
