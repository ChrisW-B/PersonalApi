{
  "name": "chriss-personal-site",
  "version": "1.0.0",
  "description": "A Simple Personal Website",
  "keywords": [
    "personal",
    "site"
  ],
  "homepage": "https://github.com/ChrisW-B/PersonalWebsite#readme",
  "bugs": {
    "url": "https://github.com/ChrisW-B/PersonalWebsite/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChrisW-B/PersonalWebsite.git"
  },
  "license": "ISC",
  "author": "Chris Barry <me@chrisb.xyz>",
  "main": "build/index.js",
  "scripts": {
    "build": "netlify-lambda build src/functions --config webpack.config.js",
    "clean": "rm -rf ./build",
    "lint:js": "eslint \"./src/**/*.{t,j}s{x,}\" --ignore-pattern 'build/**/*.js'",
    "lint:js:fix": "npm run lint:js -- --fix",
    "prettier": "prettier --list-different './**/*.{ts,js,jsx,tsx,gql,html,yml,json}' --write",
    "prettier:check": "prettier './**/*.{ts,js,jsx,tsx,gql,html,yml,json}' -c",
    "start": "netlify dev",
    "typescript": "tsc --noEmit"
  },
  "dependencies": {
    "apollo-server": "^2.19.2",
    "apollo-server-lambda": "^2.19.2",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-graphql": "^0.12.0",
    "graphql": "^15.5.0",
    "lastfm-njs": "^1.0.0",
    "node-fetch": "^2.6.1",
    "twitter": "^1.7.1",
    "twitter-text": "^3.1.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.13",
    "@babel/core": "^7.12.13",
    "@babel/node": "^7.12.13",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
    "@babel/preset-env": "^7.12.13",
    "@babel/preset-typescript": "^7.12.13",
    "@types/aws-lambda": "^8.10.71",
    "@types/node": "^14.14.25",
    "@types/node-fetch": "^2.5.8",
    "@types/twitter": "^1.7.0",
    "@types/twitter-text": "^3.1.0",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "@typescript-eslint/typescript-estree": "^4.15.0",
    "eslint": "^7.19.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "import-sort": "^6.0.0",
    "import-sort-parser-typescript": "^6.0.0",
    "import-sort-style-react": "^5.1.0",
    "netlify-cli": "^3.6.0",
    "netlify-lambda": "^2.0.3",
    "nodemon": "^2.0.7",
    "path": "^0.12.7",
    "prettier": "^2.2.1",
    "prettier-eslint": "^12.0.0",
    "prettier-plugin-import-sort": "0.0.6",
    "prettier-plugin-packagejson": "^2.2.9",
    "typescript": "^4.1.4",
    "webpack-cli": "^4.5.0",
    "webpack-node-externals": "^2.5.2"
  },
  "importSort": {
    ".js, .jsx, .es6, .es": {
      "parser": "babylon",
      "style": "react"
    },
    ".ts, .tsx": {
      "parser": "typescript",
      "style": "react"
    }
  }
}
